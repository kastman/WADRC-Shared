<h1>Listing diagnosis_methods</h1>

<table>
  <tr>
    <th>Consensus method</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @diagnosis_methods.each do |diagnosis_method| %>
  <tr>
    <td><%= diagnosis_method.consensus_method %></td>
    <td><%= link_to 'Show', diagnosis_method %></td>
    <td><%= link_to 'Edit', edit_diagnosis_method_path(diagnosis_method) %></td>
    <td><%= link_to 'Destroy', diagnosis_method, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

